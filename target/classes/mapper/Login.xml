<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="org.lzf.dao.LoginDao">

    <select id="select" parameterType="java.lang.String" resultType="org.lzf.bean.Account">
        select *
        from account
        where account = #{account};
    </select>

</mapper>